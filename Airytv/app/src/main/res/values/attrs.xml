<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DottedProgressBar">
        <attr name="emptyDotsColor" format="color" />
        <attr name="activeDotColor" format="color" />
        <attr name="maxDots" format="integer" />
        <attr name="inactiveDot" format="reference|color" />
        <attr name="activeDot" format="reference|color" />
        <attr name="dotSize" format="dimension" />
        <attr name="spacing" format="dimension" />
        <attr name="jumpingSpeed" format="integer" />
        <attr name="activeDotIndex" format="integer" />
    </declare-styleable>

    <declare-styleable name="RoundedCornerLayout">
        <attr name="radius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="PagerRecyclerView">
        <attr name="itemMargin" format="dimension" />
    </declare-styleable>

    <declare-styleable name="GiveawaysButton">
        <attr name="textColor" format="color" />
    </declare-styleable>

    <declare-styleable name="CustomMediaRouteButton">
        <attr name="customTintColor" format="color" />
    </declare-styleable>
</resources>